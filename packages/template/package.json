{
  "name": "@dsc/template",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig template/tsconfig.json",
    "check:watch": "svelte-check --tsconfig template/tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=.  && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ",
    "pages": "pnpm run build && gh-pages -d build -f -t"
  },
  "devDependencies": {
    "@dsc/configs": "workspace:*",
    "@dsc/svg": "workspace:*",
    "@dsc/utils": "workspace:*",
    "@dsc/website": "workspace:*",
    "@kahi-ui/framework": "^0.4.11",
    "@prettier/plugin-pug": "^1.17.1",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.4.0",
    "convert-hrtime": "^5.0.0",
    "gh-pages": "^3.2.3",
    "mdsvex": "^0.9.8",
    "pnpm": "latest",
    "postcss": "^8.3.9",
    "rehype-raw": "^6.1.0",
    "rehype-slug": "^5.0.0",
    "remark-directive": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.43.4",
    "svelte": "^3.34.0",
    "svelte-check": "^2.2.8",
    "svelte-preprocess": "^4.9.4",
    "svgpath": "^2.3.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite-plugin-replace": "^0.1.1"
  },
  "dependencies": {
    "node-fetch": "3.0.0",
    "node-ical": "^0.14.1",
    "sveltestrap": "^5.6.3",
    "bootstrap": "^5.1.3"
  }
}
